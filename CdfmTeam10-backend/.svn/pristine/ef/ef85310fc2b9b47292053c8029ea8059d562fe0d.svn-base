package com.gtanla.backend.exception;

/**
 * Backend exception type.
 * 
 * @author gtalbot
 * 
 */
public class BackendException extends Exception {

	/** UID. */
	private static final long serialVersionUID = 2042210775419488147L;

	/**
	 * Constructor.
	 * 
	 * @param message
	 */
	public BackendException(String message) {
		super(message);
	}

	/**
	 * Constructor.
	 * 
	 * @param cause
	 */
	public BackendException(Throwable cause) {
		super(cause);
	}

}
